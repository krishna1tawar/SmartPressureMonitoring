{
  "openapi": "3.0.4",
  "info": {
    "title": "Sensor API",
    "version": "v1"
  },
  "paths": {
    "/api/Alerts/list": {
      "get": {
        "tags": [
          "Alerts"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Alerts/resolve/{id}": {
      "post": {
        "tags": [
          "Alerts"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/Risk/predict": {
      "post": {
        "tags": [
          "Risk"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/PressureRequest"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/PressureRequest"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/PressureRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
    "/api/SensorData/latest": {
      "get": {
        "tags": [
          "SensorData"
        ],
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "PressureRequest": {
        "type": "object",
        "properties": {
          "pressure": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      }
    }
  }
}